generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  password     String
  createdAt DateTime @default(now())
}

model complaint {
  id        Int      @id @default(autoincrement())
  imgurl    String
  title     String
  desc      String
  category  String
  position  String
  city      String
  state     String
  country   String
  createdAt DateTime @default(now())
}
